/* To print pattern using numbers */
#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num;
	setbuf(stdout,NULL);
	printf("Enter the required rows :");
	scanf("%d",&num);


	return EXIT_SUCCESS;
}
