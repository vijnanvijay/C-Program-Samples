/* To print odd & even numbers upto the given range*/
#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num;
	setbuf(stdout,NULL);
	printf("To display odd & even number in a given range");
	printf("\n---------------------------------------------\n");
	printf("Enter a number : ");
	scanf("%d",&num);


	return EXIT_SUCCESS;
}
