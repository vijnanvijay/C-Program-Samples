/* To print multiplication table for the given number*/
#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num;
	setbuf(stdout,NULL);
	printf("To print Multiplication Table");
	printf("\n----------------------------\n");
	printf("Enter multiplication table of : ");
	scanf("%d",&num);




	return EXIT_SUCCESS;
}
