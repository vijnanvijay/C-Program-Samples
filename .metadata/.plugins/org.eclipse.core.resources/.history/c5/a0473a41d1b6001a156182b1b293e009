#include <stdio.h>
#include <stdlib.h>

int main(void) {
	printf("Hello world");
	int a,b,sum;
	printf("Enter 2 numbers:");
	scanf("%d%d",&a,&b);
	sum=a+b;
	printf("Result is: %d",sum);
	return EXIT_SUCCESS;
}
